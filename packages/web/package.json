{
  "name": "@medtrac/web",
  "version": "1.0.17",
  "private": true,
  "dependencies": {
    "@apollo/react-hooks": "^3.1.3",
    "@commitlint/cli": "^8.0.0",
    "@commitlint/config-conventional": "^8.0.0",
    "@material-ui/core": "^4.6.1",
    "@material-ui/icons": "^4.9.1",
    "@medtrac/common": "^1.0.17",
    "apollo-boost": "^0.4.7",
    "auth0-js": "^9.12.2",
    "clsx": "^1.1.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-react": "^7.12.4",
    "husky": "^3.0.9",
    "lerna": "^3.15.0",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.3.0",
    "typescript": "^3.7.2"
  },
  "scripts": {
    "start": "cross-env SKIP_PREFLIGHT_CHECK=true react-scripts start",
    "build": "cross-env SKIP_PREFLIGHT_CHECK=true react-scripts build",
    "test": "cross-env SKIP_PREFLIGHT_CHECK=true react-app-rewired test --runInBand",
    "eject": "react-scripts eject",
    "ci:test": "SKIP_PREFLIGHT_CHECK=true JEST_JUNIT_OUTPUT=$TEST_OUTPUT/react.xml react-scripts test --ci --runInBand --detectOpenHandles --forceExit --testResultsProcessor=jest-junit",
    "ci:deploy": "npm run heroku:deploy",
    "heroku:deploy": "git push https://heroku:$HEROKU_API_KEY@git.heroku.com/insulution-web.git master",
    "gen": "./node_modules/.bin/graphql-codegen"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@apollo/react-testing": "^3.1.3",
    "@graphql-codegen/add": "^1.12.1",
    "@graphql-codegen/cli": "^1.12.1",
    "@graphql-codegen/typescript": "^1.12.1",
    "@graphql-codegen/typescript-operations": "^1.12.1",
    "@graphql-codegen/typescript-react-apollo": "^1.12.1",
    "@graphql-codegen/typescript-resolvers": "^1.12.1",
    "@testing-library/jest-dom": "^5.1.1",
    "@testing-library/react": "^9.4.1",
    "@types/auth0-js": "^9.12.3",
    "@types/history": "^4.7.4",
    "@types/jest": "^24.0.23",
    "@types/node": "^12.12.7",
    "@types/react": "^16.9.11",
    "@types/react-dom": "^16.9.4",
    "@types/react-router-dom": "^5.1.3",
    "@types/testing-library__jest-dom": "^5.0.1",
    "@types/testing-library__react": "^9.1.2",
    "cross-env": "^5.2.0",
    "eslint": "^6.8.0",
    "prettier": "^1.19.1",
    "typescript": "^3.7.2"
  },
  "proxy": "http://localhost:3001",
  "jest": {
    "watchPathIgnorePatterns": [
      "/__utils",
      "/__mocks",
      "/generated/",
      "/.git/",
      "/.history/"
    ]
  }
}
