    "@types/node": "^11.11.2",
    "cross-env": "^5.2.0",
    "jest": "^24.5.0",
    "ts-jest": "^24.0.0",
    "ts-node-dev": "^1.0.0-pre.44"
  },
  "dependencies": {
    "@vitalic-classic/common": "^1.0.16",
